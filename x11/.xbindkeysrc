"xbacklight -inc 10"
	XF86MonBrightnessUp

"xbacklight -dec 10"
	XF86MonBrightnessDown

"sudo ~/bin/keyboard.pl up"
	XF86KbdBrightnessUp

"sudo ~/bin/keyboard.pl down"
	XF86KbdBrightnessDown

"ponymix decrease 2"
	XF86AudioLowerVolume

"ponymix increase 2"
	XF86AudioRaiseVolume

"amixer -q sset Master toggle"
	XF86AudioMute
